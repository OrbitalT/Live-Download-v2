{
  "name": "live-download-v2",
  "version": "2.0.0",
  "description": "A youtube-dl channel downloader",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . live-download --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Live-Download\"",
    "package-linux": "electron-packager . live-download --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "build": {
    "appId": "com.live.download.live-download",
    "productionName": "Live-Download"
  },
  "keywords": [
    "youtube-dl",
    "mp3",
    "mp4",
    "downloader",
    "electron",
    "npm",
    "html"
  ],
  "author": "NukeZ",
  "license": "MIT",
  "dependencies": {
    "electron": "^7.1.4",
    "node-cmd": "^3.0.0"
  }
}
